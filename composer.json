{
    "name": "rollout/rollout",
    "description": "Rollout CLI",
    "keywords": [],
    "homepage": "https://rollout.sh",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "neerajsohal",
            "email": "hello@neerajkumar.name"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel-zero/framework": "^11.0.0"
    },
    "require-dev": {
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["rollout"]
}
